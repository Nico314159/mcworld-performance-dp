{
	"projectId": "performance-dp",
	"projectName": "Performance Data Pack",
	"version": "1.0.0",
	"scripts": {
		"package": "ts-node ./.tools/package.ts",
		"setup": "ts-node ./.tools/setup.ts",
		"create_datapack": "ts-node ./.tools/create_datapack.ts",
		"scan_datapacks": "ts-node ./.tools/scan_datapacks.ts",
		"dev": "echo Try running 'dev:datapack-id' instead! Or run 'scan_datapacks' if you've added a new datapack and it's not registered.",
		"dev:datapack": "cd \"datapacks/datapack\" && mcb -w-alt -debounce-time 100",
		"dev:test": "cd \"datapacks/Benchmarker\" && mcb -w-alt -debounce-time 100",
		"dev:project-name": "cd \"datapacks/Project Name\" && mcb -w-alt -debounce-time 100"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/terminal-kit": "^2.5.1",
		"prettier": "^2.8.3"
	},
